<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <connectionStrings>
    <add name="minhaStringDeConexao" connectionString="Server=127.0.0.1;Database=CarControl;Trusted_Connection=True;" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <!-- Tempo máximo para um comando T-SQL ou stored procedure ser executado em seconds. The defaul is 30 seconds. -->
    <add key="SqlTimeout" value="60" />
    <!-- Mostra os campos Id nos forms ou não -->
    <add key="showId" value="false" />
  </appSettings>
</configuration>